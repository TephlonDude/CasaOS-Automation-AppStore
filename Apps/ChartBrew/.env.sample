# Copy this file to .env and edit values for your CasaOS deployment
# Run: cp .env.sample .env (or create a .env from this template)

# Database (MySQL) settings
CB_DB_TYPE=mysql
CB_DB_HOST=mysql
CB_DB_PORT=3306
CB_DB_NAME=chartbrew
CB_DB_USER=chartbrew
CB_DB_PASS=changeme-db
MYSQL_ROOT_PASSWORD=changeme-root

# Redis (password for runtime)
REDIS_PASSWORD=changeme-redis

# App secrets (do not use defaults in production)
CB_SECRET=changeme-secret
CB_ENCRYPTION_KEY=0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef

# App port
PORT=4018

# Optional: if you prefer a different ChartBrew image, set it here
CHARTBREW_IMAGE=chartbrew/chartbrew:v4.6.0
